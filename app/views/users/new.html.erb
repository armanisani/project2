<h1>signup to cart</h1>

<%= form_for @user, url: users_path, html: { multipart: true} do |f| %>
  <%= f.label :username, "Username" %>
  <%= f.text_field :username %>

  <%= f.label :first, "First Name" %>
  <%= f.text_field :first %>

  <%= f.label :avatar, 'Profile Photo' %>
  <%= f.file_field :avatar %>

  <%= f.submit %>
<% end %>
